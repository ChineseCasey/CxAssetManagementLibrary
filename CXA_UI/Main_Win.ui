/*QComboBox*/
QComboBox {
    border: 1px solid #aab2bd;
    border-radius: 4px;
    font-size: 14px;
	font-weight: 1000;
    padding: 5px 10px;
}

QComboBox::drop-down {
    background-color: transparent;
}

